{
    "multipart": [
      {
        "apply": {
          "model": "$NAMESPACE:$PATH/${BLOCK}_post"
        }
      },
      {
        "when": {
          "north": "true"
        },
        "apply": {
          "model": "$NAMESPACE:$PATH/${BLOCK}_side",
          "uvlock": true
        }
      },
      {
        "when": {
          "east": "true"
        },
        "apply": {
          "model": "$NAMESPACE:$PATH/${BLOCK}_side",
          "y": 90,
          "uvlock": true
        }
      },
      {
        "when": {
          "south": "true"
        },
        "apply": {
          "model": "$NAMESPACE:$PATH/${BLOCK}_side",
          "y": 180,
          "uvlock": true
        }
      },
      {
        "when": {
          "west": "true"
        },
        "apply": {
          "model": "$NAMESPACE:$PATH/${BLOCK}_side",
          "y": 270,
          "uvlock": true
        }
      }
    ]
  }
  